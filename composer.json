{
  "name": "xwildfire/react-cache-filesystem",
  "description": "Store cache items on the filesystem",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "react/cache": "^1.2",
    "react/filesystem": "^v0.1"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\React\\Cache\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "wyrihaximus/composer-update-bin-autoload-path": true
    }
  }
}
